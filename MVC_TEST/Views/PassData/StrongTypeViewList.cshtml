@using MVC_TEST.Models
@model List<Employee>

@{
    ViewBag.Title = "StrongTypeViewList";
}

<h2>StrongTypeViewList</h2>

@*<ul>
        @foreach (var item in Model)
        {
            <li>@item.Id</li>
            <li>@item.Name</li>
            <li>@item.Phone</li>
            <li>@item.Email</li>
        }
