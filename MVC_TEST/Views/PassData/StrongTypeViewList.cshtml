<div>
    <ul>
        @for (int i = 0; i < Model.Count; i++)
        {
            for (int j = i + 1; j < Model.Count; j++)
            {
                if (Model[i].Id == Model[j].Id)
                {
                    Model.RemoveAt(j);
                    j--;
                    continue;
                }
            }
            <li>@Model[i].Id</li>
            <li>@Model[i].Name</li>
            <li>@Model[i].Phone</li>
            <li>@Model[i].Email</li>
        }
    </ul>

</div>
